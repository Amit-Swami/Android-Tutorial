yout_marginTop=\"10dp\"\r\n        android:text=\"* Click to expand one of the listed features (for example, Analytics), then click the provided tutorial link (for example, Log an Analytics event).\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textSize=\"16sp\"\r\n        android:layout_marginTop=\"10dp\"\r\n        android:textColor=\"#000\"\r\n        android:text=\"* Click the Connect to Firebase button to connect to Firebase and add the necessary code to your app.\"/>\r\n    <View\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000\"\r\n        android:layout_marginTop=\"15dp\">\r\n\r\n    </View>\r\n    <View\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000\"\r\n        android:layout_marginTop=\"10dp\">\r\n\r\n    </View>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textSize=\"17dp\"\r\n        android:textColor=\"#a4c\"\r\n        android:layout_marginTop=\"15dp\"\r\n        android:text=\"Create a Firebase Project\"\r\n        android:textStyle=\"bold\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"It's time to add Firebase to your app. To do this you'll need a Firebase project and a Firebase configuration file for your app.\"\r\n        android:textColor=\"#000\"\r\n        android:textSize=\"16sp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textSize=\"16sp\"\r\n        android:layout_marginTop=\"10dp\"\r\n        android:textColor=\"#000\"\r\n        android:text=\"1. Create a Firebase project in the Firebase console, if you don't already