rginTop=\"15dp\">\r\n\r\n    </View>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"15dp\"\r\n        android:textSize=\"17dp\"\r\n        android:text=\"Database - Helper class:\"\r\n        android:textStyle=\"bold\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"For managing all the operations related to the database , an helper class has been given and is called SQLiteOpenHelper. It automatically manages the creation and update of the database. \"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"/>\r\n    <View\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000\"\r\n        android:layout_marginTop=\"15dp\">\r\n\r\n    </View>\r\n    <View\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000\"\r\n        android:layout_marginTop=\"10dp\">\r\n\r\n    </View>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textColor=\"#a4c\"\r\n        android:textSize=\"17dp\"\r\n        android:text=\"Steps to create:\"\r\n        android:textStyle=\"bold\"\r\n        android:layout_marginTop=\"15dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"1. You will use Android studio to create an Android application under a package com.example.sairamkrishna.myapplication.\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"2. Modify src/MainActivity.java file to get references of all the XML components and populate the contacts on listView.\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"3. Create new src/DBHelper.java that will manage the database work\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"4. Create a new Activity as DisplayContact.java that will display the contact on the screen\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"5. Modify the res/layout/activity_main to add respective XML components\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"6. Modify the res/layout/activity_display_contact.xml to add respective XML components\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"7. Modify the res/values/string.xml to add necessary string components\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"8. Modify the res/menu/display_contact.xml to add necessary menu components\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"9. Create a new menu as res/menu/mainmenu.xml to add the insert contact option\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"10. Run the application and choose a running android device and install the application on it and verify the results.\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000\"\r\n        android:layout_marginTop=\"10dp\"/>\r\n    <ImageView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"500dp\"\r\n        android:layout_marginTop=\"20dp\"\r\n        android:src=\"@drawable/gg58\"/>\r\n</LinearLayout>\r\n</ScrollView>
