ew\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000000\"\r\n        android:layout_marginTop=\"10dp\">\r\n\r\n    </View>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"Broadcast Receivers\"\r\n        android:textSize=\"17dp\"\r\n        android:textColor=\"#000000\"\r\n        android:textStyle=\"bold\"\r\n        android:layout_marginTop=\"15dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000000\"\r\n        android:text=\"Broadcast Receivers simply respond to broadcast messages from other applications or from the system. For example, applications can also initiate broadcasts to let other applications know that some data has been downloaded to the device and is available for them to use, so this is broadcast receiver who will intercept this communication and will initiate appropriate action.\r\n\r\nA broadcast receiver is implemented as a subclass of BroadcastReceiver class and each message is broadcaster as an Intent object.\"/>\r\n    <View\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000000\"\r\n        android:layout_marginTop=\"15dp\">\r\n\r\n    </View>\r\n    <View\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000000\"\r\n        android:layout_marginTop=\"10dp\">\r\n\r\n    </View>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:text=\"Content Providers\"\r\n        android:textSize=\"17dp\"\r\n        android:textColor=\"#000000\"\r\n        android:textStyle=\"bold\"\r\n        android:layout_marginTop=\"