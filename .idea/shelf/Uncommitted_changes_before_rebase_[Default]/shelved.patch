match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textColor=\"#000000\"\r\n        android:textSize=\"17dp\"\r\n        android:textStyle=\"bold\"\r\n        android:text=\"android:targetSDKVersion:\"\r\n        android:layout_marginTop=\"15dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000000\"\r\n        android:text=\"An integer designating the API Level that the application targets. If not set, the default value equals that given to minSdkVersion.\r\nThis attribute informs the system that you have tested against the target version and the system should not enable any compatibility behaviors to maintain your app's forward-compatibility with the target version. The application is still able to run on older versions (down to minSdkVersion).\">\r\n\r\n    </TextView>\r\n    <View\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000000\"\r\n        android:layout_marginTop=\"15dp\"/>\r\n    <View\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"1dp\"\r\n        android:background=\"#000000\"\r\n        android:layout_marginTop=\"10dp\">\r\n\r\n    </View>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textColor=\"#000000\"\r\n        android:text=\"Permissions:\"\r\n        android:textSize=\"17dp\"\r\n        android:textStyle=\"bold\"\r\n        android:layout_marginTop=\"15dp\"/>\r\n    <TextView\r\n        android:layout_width=\"match_parent\"\r\n        android:layout_height=\"wrap_content\"\r\n        android:textSize=\"16sp\"\r\n        android:textColor=\"#000000\"\r\n        android:text=\"This document describes how application developers can use the security features provided by Android.